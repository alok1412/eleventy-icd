    <link rel="stylesheet" type="text/css" href="/static/css/globals.css"/>
    <link rel="stylesheet" type="text/css" href="/static/css/modules/header.css"/>
</head>
<header id="header">
    <div class="menu-cont">
        <span class="yellowLine">
            <span class="loader"></span>
        </span>
        <div class="container">
            <div class="row">
                <div class="col-10 col-md-2 logo-container">
                    <a href="/" aria-label="logo" class="logo d-none d-lg-block">
                        <img decoding="async" width="172" height="43" src="/static/assets/logo/icd-logo.9e81fca5.svg" class="logo d-none d-lg-block" alt="icd-logo" />
                    </a>
                    <a href="/" aria-label="logo" class="logo d-block d-lg-none">
                        <img decoding="async" src="/static/assets/logo/mobile-logo-new.png" width="48" height="36"  class="logo d-block d-lg-none" alt="icd-logo" />
                    </a>
                </div>
                <div class="col-2 col-md-10">
                    <div class="d-block d-lg-none">

                        <div class="hamburger hamburger--spring js-hamburger" onclick="hamburgerToggle()">
                            <div class="hamburger-box">
                                <div class="hamburger-inner"></div>
                            </div>
                        </div>

                    </div>
                    <div class="nav-menu">
                        <div class="container">
                            <div class="row">
                                <ul>
                                    <li class="mobile__menu--items" onclick="hamburgerClose()"><a href="/">home</a></li>
                                    <li onclick="hamburgerClose()" ><a href="/projects">projects</a></li>
                                    <li onclick="hamburgerClose()" ><a href="/clients">clients</a></li>
                                    <li onclick="hamburgerClose()" ><a href="/services">services</a></li>
                                    <li onclick="hamburgerClose()" ><a href="/posts">posts</a></li>
                                    <li onclick="hamburgerClose()" ><a href="/contact">contact</a></li>
                                    <li onclick="hamburgerClose()" class="mobile__menu--items"><a href="/our-team">team</a></li>
                                    <li onclick="hamburgerClose()" class="mobile__menu--items"><a href="/careers">careers</a></li>
                                    <li onclick="hamburgerClose()" class="copyright">© 1990-2019 itu chaudhuri design pvt ltd | all rights reserved. please note — no images or content from site can be reproduced without prior written consent from icd</li>
                                </ul>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
                  

    <div id="close">
        <span class="close-wrap" onClick={this.searchToggle}>
            <span class="close-line close-line1"></span>
            <span class="close-line close-line2"></span>
        </span>
    </div>
</header>

<script>
    function hamburgerToggle() {
        $('body').toggleClass('hamburger-open');
        $('.hamburger, .nav-menu').toggleClass("is-active");
    }

    function hamburgerClose() {
        $('body').removeClass('hamburger-open');
        $('.hamburger, .nav-menu').removeClass("is-active");
    }
    
</script>